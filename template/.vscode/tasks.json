{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Story HTML file",
			"type": "shell",
			"command": "tweego -o .\\${fileBasenameNoExtension}.html .\\src ",
			"group": "build",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": true,
				"panel": "new" 
			}
		}
	]
}